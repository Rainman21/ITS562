{
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "Run Node on server.js",
            "program": "${workspaceFolder}/nodeServer/server.js"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "AjaxTest Chrome",
            "url": "http://localhost:8080/ajaxworkbench.html",
            "webRoot": "${workspaceRoot}",
            "sourceMaps": true
        }
    ]
}